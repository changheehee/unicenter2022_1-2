package Hehehe;
import java.util.Scanner;

public class Hehehehe{
	public static void main(String[]args){
		Scanner sc = new Scanner(System.in);
		int value = sc.nextInt(), number = 0;
		
		if (value <= 100) {
			for (int i = 1; i <= 100; i++) {
				number = i * value;
				System.out.print(number + " ");
				if ((number%10) == 0)	break;
			}
		}
	}
}

