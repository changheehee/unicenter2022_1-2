package Hehehe;
import java.util.Scanner;

public class Hehehehe{
	public static void main(String[]args){
		Scanner sc = new Scanner(System.in);
		int value = sc.nextInt();
		
		for (int i = 0; i < value; i++){
			for (int k = value-1; k > i; k--){
				System.out.print("  ");
			}
			for (int j = 1; j <= i+1; j++){
				System.out.print(j+" ");
			}
			System.out.println();
		}
	}
}

