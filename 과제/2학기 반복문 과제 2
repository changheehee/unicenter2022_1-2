package Hehehe;
import java.util.Scanner;

public class Hehehehe{
	public static void main(String[]args){
		Scanner sc = new Scanner(System.in);
		int value = sc.nextInt(), plus = 0;
		
		for (int i = 0; i < value; i++){
			for (int k = 0; k < i; k++){
				System.out.print("  ");
			}
			for (int j = value; j > i; j--){
				plus++;
				System.out.print(plus+" ");
			}
			System.out.println();
		}
	}
}

___________________________

package Hehehe;
import java.util.Scanner;

public class Hehehehe{
	public static void main(String[]args){
		Scanner sc = new Scanner(System.in);
		int value = sc.nextInt(), count = 0;
		int[] holsu = {1,3,5,7,9};
		
		for (int i = 0; i < value; i++) {
			for (int j = 0; j < value; j++) {
				if (count == 5) count = 0;
				System.out.print(holsu[count++]+" ");		
			}
			System.out.println();
		}
	}
}
